<Application x:Class="wpfLabs.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wpfLabs"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
			<ResourceDictionary>
				<ResourceDictionary.MergedDictionaries>
					<ResourceDictionary Source="Resources/Dictionary/lang.en-US.xaml" />
					<ResourceDictionary Source="Resources/Dictionary/lang.ru-RU.xaml" />
					<ResourceDictionary Source="Resources/ResourceDict/Styles.xaml" />
				</ResourceDictionary.MergedDictionaries>
			</ResourceDictionary>
	</Application.Resources>
</Application>
